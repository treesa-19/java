#include <stdio.h>

int main()
{
float mark;
char garde;

printf("Enter the mark");
scanf("%f",&mark);

if(mark>=90)
{
    printf("grade is A");
}else if(mark>=80 && mark<=89)
{
    printf("grade is B");
}else if(mark>=70 && mark<=79)
{
    printf("grade is C");
}else if(mark>=60 && mark<=69)
{
    printf("grade is D");
}else if(mark>=50 && mark<=59)
{
    printf("garde is E");
}else
{
    printf("failed");
}

return 0;
}



